.dataset-show
  .heading-wrapper.clearfix
    %h2= @dataset.name
    .create-run= button_to "Run MOCK on this Dataset", create_run_path(:dataset => @dataset.id), :class => "orange-button", :onclick => "return confirm('Are you sure you want to run MOCK on #{@dataset.name}?')"

  %table
    - @dataset.datapoints.each do |datapoint|
      %tr
        - datapoint.datavalues.each do |datavalue|
          %td= "#{datavalue.value} "